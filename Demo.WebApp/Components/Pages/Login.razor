@layout LoginLayout;
@inherits UnqPageBase
@page "/Login"
@rendermode InteractiveServer


@if (ShowNewLoginForm)
{
    <NewLoginComponent OnCancelClick="@NewLoginCoponent_OnCancel" OnRegisterClick="@NewLoginCoponent_OnRegister"></NewLoginComponent>
}
else
{
    <LoginComponent OnRegisterClick="@LoginComponent_OnRegister" OnLoginClick="@LoginComponent_OnLogin"></LoginComponent>
}
